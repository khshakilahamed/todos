									"let response = pm.response.json();\r",
									"\r",
									"// Check if tokens exist\r",
									"if (response.access && response.refresh) {\r",
									"    // Store both tokens in environment variables\r",
									"    pm.environment.set(\"access_token\", response.access);\r",
									"    pm.environment.set(\"refresh_token\", response.refresh);\r",
